{
    "name": "Console Save",
    "description": "Build an Extension!",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["storage", "activeTab", "scripting"],
    "background": {},
    "content_scripts": [
        {
            "matches": ["https://*/*", "http://*/*"],
            "js": ["content-script.js"]
        }
    ],
    "web_accessible_resources": [
        {
            "resources": ["script.js"],
            "matches": ["https://*/*", "http://*/*"]
        }
    ],
    "icons": {
        "16": "/icons/16.png",
        "32": "/icons/32.png",
        "48": "/icons/48.png",
        "128": "/icons/128.png"
    }
}
